<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox prefHeight="270.0" prefWidth="600.0" styleClass="background-black" stylesheets="@../style.css" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="CRUD.Audit_logsDialogController">
   <children>
      <Label fx:id="Audit_logsOrderLabel" styleClass="font-white" stylesheets="@../style.css" text="Create Audit_logs">
         <font>
            <Font size="24.0" />
         </font>
      </Label>
      <TextField fx:id="user_idTextField" prefHeight="25.0" prefWidth="575.0" styleClass="textfield-design" stylesheets="@../style.css" />
      <Label prefWidth="38.0" styleClass="font-white" stylesheets="@../style.css" text="user_id" />
      <TextField fx:id="action_typeTextField" prefHeight="25.0" prefWidth="575.0" styleClass="textfield-design" stylesheets="@../style.css" />
      <Label styleClass="font-white" stylesheets="@../style.css" text="action_type" />
      <TextField fx:id="descriptionTextField" prefHeight="25.0" prefWidth="575.0" styleClass="textfield-design" stylesheets="@../style.css" />
      <Label styleClass="font-white" stylesheets="@../style.css" text="description" />
      <TextField fx:id="ip_addressTextField" prefHeight="25.0" prefWidth="575.0" styleClass="textfield-design" stylesheets="@../style.css" />
      <Label styleClass="font-white" stylesheets="@../style.css" text="ip_address" />
      <Pane maxHeight="-Infinity" prefHeight="20.0" prefWidth="200.0" />
      <HBox>
         <children>
            <Button mnemonicParsing="false" onAction="#handleSave" prefHeight="25.0" prefWidth="110.0" styleClass="button-design" stylesheets="@../style.css" text="Save" />
            <Pane prefHeight="20.0" prefWidth="200.0" HBox.hgrow="SOMETIMES" />
            <Button mnemonicParsing="false" onAction="#handleCancel" prefHeight="25.0" prefWidth="110.0" styleClass="button-design" stylesheets="@../style.css" text="Cancel" />
         </children>
         <padding>
            <Insets left="20.0" right="20.0" />
         </padding>
      </HBox>
   </children>
   <padding>
      <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
   </padding>
</VBox>
